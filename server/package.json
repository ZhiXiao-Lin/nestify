    "license": "MIT",
    "scripts": {
        "prod": "cross-env NODE_ENV=production pm2 start dist/main.js --name 'nestify:8008'",
        "stop": "pm2 delete nestify:8008",
        "orm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "db:create": "cd ./database && cross-env PGPASSWORD=123456 psql -U postgres -f create-db.sql && cross-env PGPASSWORD=123456 psql -U nestify -d nestify -f create-exts.sql",
        "db:init": "yarn db:drop && yarn db:sync && yarn db:seed",
        "db:drop": "yarn orm schema:drop",
        "db:sync": "yarn orm schema:sync",
        "db:seed": "cross-env DB_INIT=true ts-node -r tsconfig-paths/register src/main.ts",
        "build": "run-s build:**",
        "build:server": "rimraf dist && tsc -p tsconfig.build.json",
        "build:next": "next build",
        "dev": "nodemon"
    },
    "dependencies": {
        "@nestjs/common": "^6.0.0",
