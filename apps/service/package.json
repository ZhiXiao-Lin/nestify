{
  "name": "service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "cli": "nest build && node dist/console",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestify/config": "0.3.19",
    "@nestify/console": "0.3.6",
    "@nestify/core": "0.3.19",
    "@nestify/crypt": "0.3.10",
    "@nestify/event-bus": "0.3.19",
    "@nestify/logger": "0.3.19",
    "@nestify/mongo-seeder": "0.3.19",
    "@nestify/notification": "0.3.19",
    "@nestjs/common": "6.10.7",
    "@nestjs/core": "6.10.7",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/platform-express": "6.10.7",
    "cache-manager": "2.10.1",
    "cache-manager-ioredis": "1.0.1",
    "mongoose": "5.7.14",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "6.12.9",
    "@nestjs/schematics": "6.7.5",
    "@nestjs/testing": "6.10.7",
    "@types/express": "4.17.2",
    "@types/faker": "4.1.8",
    "@types/jest": "24.0.23",
    "@types/mongoose": "5.5.32",
    "@types/node": "12.12.14",
    "@types/supertest": "2.0.8",
    "faker": "4.1.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
